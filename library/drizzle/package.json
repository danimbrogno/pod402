{
  "name": "@project/drizzle",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc -p tsconfig.lib.json",
    "db:generate": "dotenv-flow -p ../../ -- drizzle-kit generate",
    "db:migrate": "dotenv-flow -p ../../ -- drizzle-kit migrate",
    "db:push": "dotenv-flow -p ../../ -- drizzle-kit push",
    "db:studio": "dotenv-flow -p ../../ -- drizzle-kit studio"
  },
  "dependencies": {
    "drizzle-orm": "^0.44.7"
  },
  "devDependencies": {
    "dotenv": "^16.4.5",
    "dotenv-flow": "^4.1.0",
    "drizzle-kit": "^0.31.7",
    "typescript": "^5.4.5"
  }
}
