    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^4.3.1",
    "tslib": "^2.6.2",
    "turbo": "^2.0.0",
    "typescript": "^5.9.2",
    "vitest": "^2.0.5"
  }
}
